{
  "name": "otmjka-ui-components",
  "version": "1.0.9",
  "description": "react, material ui, storybook, ui components library",
  "type": "module",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && node esbuild.config.js && tsc --emitDeclarationOnly",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -- -o ./docs --quiet",
    "test": "vitest run --coverage",
    "test-dev": "vitest",
    "prepublishOnly": "npm run build",
    "lint": "eslint ",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@emotion/styled": "^11.13.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.15.0",
    "@fontsource/material-icons": "^5.1.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@mui/styled-engine-sc": "^6.1.5",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/addon-themes": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-react": "^4.3.3",
    "esbuild": "^0.19.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "glob": "^10.3.10",
    "globals": "^15.12.0",
    "otmjka-eslint-prettier-config": "^1.0.0",
    "prettier": "^3.3.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.53.1",
    "storybook": "^8.3.6",
    "styled-components": "^6.1.13",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.15.0",
    "vitest": "^2.1.4"
  }
}
